<MudCard Outlined Class="d-flex flex-nowrap " Style="@($"height:{Height};")">
    <MudPaper Class="flex-grow-0 p-3" Elevation="0">
        <MudText Typo=Typo.h5>Total net worth</MudText>
        <MudText Typo=Typo.h3 class="card-subtitle text-muted">
            @if (_totalNetWorth is not null)
            {
                @_totalNetWorth@_currency
            }
            else
            {
                @("No data")
            }
        </MudText>
    </MudPaper>
</MudCard>